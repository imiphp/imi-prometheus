{
    "name": "imiphp/imi-prometheus",
    "type": "library",
    "license": "MulanPSL-2.0",
    "description": "imi 框架普罗米修斯（Prometheus）服务监控指标组件。imi Prometheus component.",
    "require": {
        "imiphp/imi-meter": "~2.1.0",
        "promphp/prometheus_push_gateway_php": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~3.4.0",
        "phpstan/phpstan": "~1.8.0",
        "phpunit/phpunit": ">=9",
        "rector/rector": "^0.12.10",
        "imiphp/imi": "2.1.x-dev",
        "imiphp/imi-swoole": "2.1.x-dev",
        "imiphp/imi-workerman": "2.1.x-dev",
        "imiphp/imi-fpm": "2.1.x-dev",
        "swoole/ide-helper": "^4.8",
        "yurunsoft/yurun-http": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Imi\\Prometheus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "app\\": "example/",
            "Imi\\Prometheus\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit -c ./phpunit.xml"
